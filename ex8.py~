formatter = "%r %r %r %r"

print formatter % (1, 2, 3, 4)
print formatter % ("one", "two", "three", "four")
print formatter % (True, False, False, True)
print formatter % (formatter, formatter, formater, formater)
print formatter % ("I had this thing.", "That you could type up right.", "But id did't sing.", "So I said goodnight")
